# ~/.zsh_aliases
alias ll='ls -lah'
alias gs='git status'
alias gp='git pull'
alias gc='git commit -m'
alias ss='source ~/.zshrc'
alias colimastart='colima start --cpu 6 --memory 12'
alias ssl='NODE_OPTIONS=--dns-result-order=ipv4first local-ssl-proxy --source 3001 --target 3000'

# ping with timestamp, defaulting to google.com
pp() {
  ping "${1:-google.com}" | ts
}
